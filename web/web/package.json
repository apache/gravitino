{
  "name": "gravitino-web",
  "private": true,
  "engines": {
    "node": "20.19.0",
    "npm": ">=9.8.1"
  },
  "scripts": {
    "analyze": "env-cmd -f .env.analyze pnpm build",
    "dev": "BUILD_TARGET=new node ./scripts/select-build-target.js && next dev",
    "dev:old": "BUILD_TARGET=old node ./scripts/select-build-target.js && next dev",
    "build": "BUILD_TARGET=new node ./scripts/select-build-target.js && pnpm format && next build",
    "build:old": "BUILD_TARGET=old node ./scripts/select-build-target.js && pnpm format && next build",
    "dist": "BUILD_TARGET=new node ./scripts/select-build-target.js && BUILD_TARGET=new env-cmd -f .env.static next build",
    "dist:old": "BUILD_TARGET=old node ./scripts/select-build-target.js && BUILD_TARGET=old env-cmd -f .env.static next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "gen:icons": "tsx src/lib/icons/iconify-icons.js && prettier --write src/lib/icons/iconify-icons.css",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.24.0",
    "@ant-design/icons": "^6.1.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.7.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "5.0.0-alpha.176",
    "@mui/material": "^5.16.14",
    "@mui/x-data-grid": "^6.20.3",
    "@reduxjs/toolkit": "^1.9.7",
    "antd": "^5.24.5",
    "axios": "^1.12.0",
    "chroma-js": "^2.4.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.552.0",
    "next": "14.2.32",
    "next-themes": "^0.4.6",
    "nprogress": "^0.2.0",
    "oidc-client-ts": "^3.3.0",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-antd-column-resize": "^1.0.3",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^8.1.3",
    "react-use": "^17.6.0",
    "tailwind-merge": "^3.3.1",
    "use-resize-observer": "^9.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.1",
    "@iconify/tools": "^4.1.2",
    "@iconify/utils": "^2.1.25",
    "@next/bundle-analyzer": "^14.2.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.9",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.3.3",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "env-cmd": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.6",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "vitest": "^2.1.9"
  }
}
