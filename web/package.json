{
  "name": "gravitino-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "env-cmd -f .env.static next build",
    "lint": "eslint --fix \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "build:icons": "node src/iconify-bundle/bundle-icons-react.js",
    "rm:cache": "rm -rf ./out/ && rm -rf ./node_modules/ && rm -rf ./yarn.lock && yarn cache clean",
    "deploy": "yarn install && yarn export",
    "docker:export": "yarn deploy",
    "docker:dev": "yarn install && yarn dev"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.3.2",
    "@iconify/react": "4.1.1",
    "@mui/lab": "5.0.0-alpha.153",
    "@mui/material": "5.14.18",
    "@mui/system": "5.14.18",
    "@mui/x-data-grid": "6.18.1",
    "@mui/x-tree-view": "^6.17.0",
    "@reduxjs/toolkit": "1.9.7",
    "axios": "1.6.1",
    "axios-mock-adapter": "1.22.0",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "dayjs": "^1.11.10",
    "next": "^14.0.2",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-hot-toast": "2.4.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-popper": "2.3.0",
    "react-redux": "8.1.3",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/eslint-parser": "7.23.3",
    "@iconify/iconify": "3.1.1",
    "@iconify/json": "2.2.142",
    "@iconify/tools": "3.0.6",
    "@iconify/types": "2.0.0",
    "@iconify/utils": "2.1.11",
    "env-cmd": "^10.1.0",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "prettier": "3.1.0"
  },
  "resolutions": {
    "@mui/x-data-grid/@mui/system": "5.4.1",
    "react-hot-toast/goober/csstype": "3.0.10",
    "@emotion/react/@emotion/babel-plugin/@babel/core": "7.0.0",
    "@emotion/react/@emotion/babel-plugin/@babel/plugin-syntax-jsx/@babel/core": "7.0.0"
  }
}
